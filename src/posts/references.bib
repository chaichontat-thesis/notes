  pmid = {33355120},
  file = {/Users/chaichontat/Library/Mobile Documents/com~apple~CloudDocs/Apps/ZoteroFiles/PhD/Fu et al_2020_Predicting transcription factor binding in single cells through deep learning.pdf}
