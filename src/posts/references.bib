@article{pilazProlongedMitosis2016,
  title = {Prolonged Mitosis of Neural Progenitors Alters Cell Fate in the Developing Brain},
  author = {Pilaz, Louis-Jan and McMahon, John J. and Miller, Emily E. and Lennox, Ashley L. and Suzuki, Aussie and Salmon, Edward and Silver, Debra L.},
