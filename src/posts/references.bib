  keywords = {Aadult hippocampal neurogenesis,Aging,Models of neurogenesis,Neuroplasticity,Preservation and productivity of the stem cell pool,Self-renewal,Stem cell quiescence,Symmetric and asymmetric divisions},
  file = {/Users/chaichontat/Library/Mobile Documents/com~apple~CloudDocs/Apps/ZoteroFiles/PhD/Lazutkin et al_2019_Modes of division and differentiation of neural stem cells.pdf}
}

@article{lopezEnhancingScientific2020,
