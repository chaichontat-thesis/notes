@article{kimNuclearOligo2022,
  title = {Nuclear Oligo Hashing Improves Differential Analysis of Single-Cell {{RNA-seq}}},
  author = {Kim, Hyeon-Jin and Booth, Greg and Saunders, Lauren and Srivatsan, Sanjay and {McFaline-Figueroa}, Jos{\'e} L. and Trapnell, Cole},
