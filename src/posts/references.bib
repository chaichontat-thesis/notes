@article{katzenelenbogenCoupledScRNASeq2020,
  title = {Coupled {{scRNA-Seq}} and {{Intracellular Protein Activity Reveal}} an {{Immunosuppressive Role}} of {{TREM2}} in {{Cancer}}},
