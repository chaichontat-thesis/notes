@article{datlingerUltrahighthroughputSinglecell2021,
  title = {Ultra-High-Throughput Single-Cell {{RNA}} Sequencing and Perturbation Screening with Combinatorial Fluidic Indexing},
  author = {Datlinger, Paul and Rendeiro, Andr{\'e} F. and Boenke, Thorina and Senekowitsch, Martin and Krausgruber, Thomas and Barreca, Daniele and Bock, Christoph},
  year = {2021},
  month = jun,
  journal = {Nat Methods},
  volume = {18},
  number = {6},
  pages = {635--642},
  publisher = {{Nature Publishing Group}},
  issn = {1548-7105},
  doi = {10.1038/s41592-021-01153-z},
  abstract = {Cell atlas projects and high-throughput perturbation screens require single-cell sequencing at a scale that is challenging with current technology. To enable cost-effective single-cell sequencing for millions of individual cells, we developed `single-cell combinatorial fluidic indexing' (scifi). The scifi-RNA-seq assay combines one-step combinatorial preindexing of entire transcriptomes inside permeabilized cells with subsequent single-cell RNA-seq using microfluidics. Preindexing allows us to load several cells per droplet and computationally demultiplex their individual expression profiles. Thereby, scifi-RNA-seq massively increases the throughput of droplet-based single-cell RNA-seq, and provides a straightforward way of multiplexing thousands of samples in a single experiment. Compared with multiround combinatorial indexing, scifi-RNA-seq provides an easy and efficient workflow. Compared to cell hashing methods, which flag and discard droplets containing more than one cell, scifi-RNA-seq resolves and retains individual transcriptomes from overloaded droplets. We benchmarked scifi-RNA-seq on various human and mouse cell lines, validated it for primary human T cells and applied it in a highly multiplexed CRISPR screen with single-cell transcriptome readout of T cell receptor activation.},
  copyright = {2021 The Author(s), under exclusive licence to Springer Nature America, Inc.},
  langid = {english},
  keywords = {Functional genomics,Gene expression,Genetic engineering,RNA sequencing,Transcriptomics},
  file = {/Users/chaichontat/Library/Mobile Documents/com~apple~CloudDocs/Apps/ZoteroProfiles/PhD/Data/storage/C6773AU9/Datlinger et al. - 2021 - Ultra-high-throughput single-cell RNA sequencing a.pdf}
}

@article{dingSystematicComparison2020,
  title = {Systematic Comparison of Single-Cell and Single-Nucleus {{RNA-sequencing}} Methods},
  author = {Ding, Jiarui and Adiconis, Xian and Simmons, Sean K. and Kowalczyk, Monika S. and Hession, Cynthia C. and Marjanovic, Nemanja D. and Hughes, Travis K. and Wadsworth, Marc H. and Burks, Tyler and Nguyen, Lan T. and Kwon, John Y. H. and Barak, Boaz and Ge, William and Kedaigle, Amanda J. and Carroll, Shaina and Li, Shuqiang and Hacohen, Nir and {Rozenblatt-Rosen}, Orit and Shalek, Alex K. and Villani, Alexandra-Chlo{\'e} and Regev, Aviv and Levin, Joshua Z.},
  year = {2020},
  month = jun,
  journal = {Nat Biotechnol},
  volume = {38},
  number = {6},
  pages = {737--746},
  publisher = {{Nature Publishing Group}},
  issn = {1546-1696},
  doi = {10.1038/s41587-020-0465-8},
  abstract = {The scale and capabilities of single-cell RNA-sequencing methods have expanded rapidly in recent years, enabling major discoveries and large-scale cell mapping efforts. However, these methods have not been systematically and comprehensively benchmarked. Here, we directly compare seven methods for single-cell and/or single-nucleus profiling\textemdash selecting representative methods based on their usage and our expertise and resources to prepare libraries\textemdash including two low-throughput and five high-throughput methods. We tested the methods on three types of samples: cell lines, peripheral blood mononuclear cells and brain tissue, generating 36 libraries in six separate experiments in a single center. To directly compare the methods and avoid processing differences introduced by the existing pipelines, we developed scumi, a flexible computational pipeline that can be used with any single-cell RNA-sequencing method. We evaluated the methods for both basic performance, such as the structure and alignment of reads, sensitivity and extent of multiplets, and for their ability to recover known biological information in the samples.},
  copyright = {2020 The Author(s), under exclusive licence to Springer Nature America, Inc.},
  langid = {english},
  keywords = {Bioinformatics,Gene expression profiling,Genome-wide analysis of gene expression,RNA sequencing,Transcriptomics},
  file = {/Users/chaichontat/Library/Mobile Documents/com~apple~CloudDocs/Apps/ZoteroProfiles/PhD/Data/storage/PFQKZNLD/Ding et al. - 2020 - Systematic comparison of single-cell and single-nu.pdf}
}

