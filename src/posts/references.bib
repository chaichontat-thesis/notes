@incollection{ferreiraEDU5Ethynyl22021,
  title = {{{EDU}} (5-{{Ethynyl-2}}{${'}$}-{{Deoxyuridine}})-{{Coupled Fluorescence-Intensity Analysis}}: {{Determining Absolute Parameters}} of the {{Cell Cycle}}},
  shorttitle = {{{EDU}} (5-{{Ethynyl-2}}{${'}$}-{{Deoxyuridine}})-{{Coupled Fluorescence-Intensity Analysis}}},
  booktitle = {Cell {{Cycle Oscillators}} : {{Methods}} and {{Protocols}}},
  author = {Ferreira, Jo{\~a}o A. and Neves, Marco and Alpalh{\~a}o, Miguel and Pereira, Pedro and Cunha, Daniela and Ferreira, Fernando and Santus, Ren{\'e} and Sousa, Ana E. and Filipe, Paulo L.},
  editor = {Coutts, Amanda S. and Weston, Louise},
  year = {2021},
  series = {Methods in {{Molecular Biology}}},
  pages = {165--177},
  publisher = {{Springer US}},
  address = {{New York, NY}},
  doi = {10.1007/978-1-0716-1538-6_12},
  abstract = {The principles and practice of a methodology of cell cycle analysis that allows the estimation of the absolute length (in units of time) of all cell cycle stages (G1, S, and G2) are detailed herein. This methodology utilizes flow cytometry to take full advantage of the excellent stoichiometric properties of click chemistry. This allows detection, via azide-fluorochrome coupling, of the modified deoxynucleoside 5-ethynyl-2{${'}$}-deoxyuridine (EDU) incorporated into replicated DNA through incremental pulsing times. This methodology, which we designated as EdU-Coupled Fluorescence Intensity (E-CFI) analysis, can be applied to cell types with very distinct cell cycle features, and has shown excellent agreement with established techniques of cell cycle analysis. Useful modifications to the original protocol (Pereira et al., Oncotarget, 8:40514\textendash 40,532, 2017) have been introduced to increase flexibility in data collection and facilitate data analysis.},
  isbn = {978-1-07-161538-6},
  langid = {english},
  keywords = {Absolute length (units of time),Cell cycle phases,E-CFI,Flow cytometry}
}

@article{fuPredictingTranscription2020,
  title = {Predicting Transcription Factor Binding in Single Cells through Deep Learning},
