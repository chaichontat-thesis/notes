@article{chanGeneRegulatory2017,
  title = {Gene {{Regulatory Network Inference}} from {{Single-Cell Data Using Multivariate Information Measures}}},
  author = {Chan, Thalia E. and Stumpf, Michael P. H. and Babtie, Ann C.},
  year = {2017},
